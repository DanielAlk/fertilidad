<%= render 'slider' %>
<section>
	<div class="container">
		<h2>IMÁGENES<a href="<%= image_galleries_path %>" class="btn btn-primary pull-right">VER MÁS</a></h2>
		<div class="row tight-10 space-NS-40 organize-S-20">
			<% @image_galleries.each do |gallery| %>
				<div class="col-xs-6 col-sm-3">
					<%= link_to gallery, class: 'clean' do %>
						<%= image_tag gallery.cover.url(:thumb), class: 'cover' %>
					<% end %>
				</div>
			<% end %>
		</div>
	</div>
</section>
<section class="bg-couple-coffee">
	<div class="container home-practices">
		<div class="row">
			<% letters = ('a'...'z').to_a %>
			<% PracticeType.all.each_with_index do |practice_type, index| %>
				<div class="col-sm-6">
					<% if index == 0 %>
						<h2>PRÁCTICAS</h2>
					<% else %>
						<h2>&nbsp;</h2>
					<% end %>
					<h3 class="text-uppercase"><%= letters[index] + ') ' + practice_type.title %></h3>
					<ul>
						<% info_count = 0 %>
						<% info_collection = [] %>
						<% practice_type.practices.each do |practice| %>
							<li>
								<%= practice.title %>
								<% if practice.info.present? %>
									<% if info_collection.include? practice.info %>
										<sup><%= info_collection.index(practice.info) +1 %></sup>
									<% else %>
										<sup><%= info_count+=1 %></sup>
										<% info_collection << practice.info %>
									<% end %>
								<% end %>
							</li>
						<% end %>
					</ul>
					<% info_collection.each_with_index do |info, info_index| %>
						<br><div><small><sup><%= info_index + 1 %></sup> <%= info %></small></div>
					<% end %>
				</div>
			<% end %>
		</div>
	</div>
</section>
